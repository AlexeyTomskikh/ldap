@{
    ViewBag.Title = "Расписание";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>

    @* jQuery *@
    <script src="~/Scripts/MyCalendar/jQuery/jquery-3.2.1.js"></script>
    <link href="~/Scripts/jquery-ui-1.12.1/jquery-ui-1.12.1/jquery-ui.css" rel="stylesheet" />
    <link href="~/Scripts/jquery-ui-1.12.1/jquery-ui-1.12.1/jquery-ui.theme.css" rel="stylesheet" />


    <script src="@Url.Content("~/Scripts/jquery-ui-1.12.1.min.js")"
            type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-timepicker-addon.js")"
            type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/JavaScript1.js")"
            type="text/javascript"></script>
    <link href="@Url.Content("~/Scripts/jquery-ui-timepicker-addon.min.css")"
          rel="stylesheet" type="text/css" />


    @*Мои стили*@
    <link href="~/Content/css/main.css" rel="stylesheet" />
    <link href="~/Content/css/calendar-view.css" rel="stylesheet" />
    <link href="~/Content/css/add-event-form.css" rel="stylesheet" />
    <link href="~/Content/css/schedule-view.css" rel="stylesheet" />

    @* Скрипт для удаления событий *@
    <script src="~/Scripts/js/RemoveEvent.js"></script>

    @* Скрипт для добавления событий *@
    <script src="~/Scripts/js/AddEvent.js"></script>

    @* Датапикеры на формы ввода *@
    <script src="~/Scripts/js/datepicker-setup.js"></script>

    @* Скрипт управляет обновлением данных на странице *@
    <script src="~/Scripts/js/reload-elements.js"></script>

</head>
<body>
    @RenderBody()
</body>
</html>
