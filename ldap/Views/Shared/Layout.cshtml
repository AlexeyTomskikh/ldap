    ViewBag.Title = "Расписание";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    @* jQuery *@
    <script src="~/Scripts/MyCalendar/jQuery/jquery-3.2.1.js"></script>
    <link href="~/Scripts/jquery-ui-1.12.1/jquery-ui-1.12.1/jquery-ui.css" rel="stylesheet" />
    <link href="~/Scripts/jquery-ui-1.12.1/jquery-ui-1.12.1/jquery-ui.theme.css" rel="stylesheet" />


    <script src="@Url.Content("~/Scripts/jquery-ui-1.12.1.min.js")"
            type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-timepicker-addon.js")"
            type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/JavaScript1.js")"
            type="text/javascript"></script>
    <link href="@Url.Content("~/Scripts/jquery-ui-timepicker-addon.min.css")"
          rel="stylesheet" type="text/css" />
 
    <link href="@Url.Content("~/Content/themes/base/all.css")" rel="stylesheet" type="text/css" />

    @*Верификация формы добавления события*@
    <script src="~/Scripts/js/VerificationAddForm.js"></script>

    @*Скрипт вывода модального окна "Добавить событие"*@
    @*<script src="~/Scripts/myScript.js"></script>*@

   

    @*bootstrap*@
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-theme.min.css" rel="stylesheet" />
    <script src="~/Scripts/bootstrap.min.js"></script>
</head>
<body>




    @*<div>
        <script src="~/Scripts/bootstrap.min.js"></script>
        @*<script src="~/Scripts/jquery-1.9.1.min.js"></script>
        <script src="~/Scripts/bootstrap.js"></script>
        <script src="~/Scripts/jquery-1.9.1.js"></script>
        <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    </div>*@

    @* Скрипт навешивает датапикеры на формы ввода *@
    <script type="text/javascript">
        $(document).ready(function () {
            $("input[name='StartEvent']").datetimepicker();
            $("input[name='EndEvent']").datetimepicker();
            $("input[name='StartEvent1']").datetimepicker();
            $("input[name='EndEvent1']").datetimepicker();
            $("input[name='FilterStartEvent']").datepicker();
            $("input[name='FilterEndEvent']").datepicker();
        });
    </script>

    @* Скрипт принимает данные от контроллера , в ответ на отправку ajax формы  *@




    @RenderBody()
</body>
</html>
